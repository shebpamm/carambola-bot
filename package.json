{
  "name": "carambola-discord",
  "version": "0.0.1",
  "description": "A discord bot made mainly for organizing 10-man pugs.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "shebpamm",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.4.0",
    "form-data": "^3.0.0",
    "mongoose": "^5.10.9",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "xo": "^0.34.1"
  },
  "xo": {
    "globals": [
      "__basedir"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "guard-for-in": "off",
      "unicorn/no-for-loop": "off",
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "kebabCase": true
          }
        }
      ]
    }
  }
}
